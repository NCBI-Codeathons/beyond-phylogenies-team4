#!/usr/bin/env python

import sys
import csv

def main(filename):
    with open(filename, "r") as f:
        c = csv.reader(f, delimiter='\t')
        for row in c:
            name = row[0]
            gisaid = row[1]
            if "Miami" in name:
                nename= name.replace("Miami", "MSL")
            sys.stdout.write(f"{gisaid}\t{name}\t{newname}\n")

if __name__ == "__main__":
    main(sys.argv[1])
